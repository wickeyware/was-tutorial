{
  "name": "was-tutorial",
  "version": "1.1.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:all && npm run serve:cli",
    "buildgh": "npm run build:github && npm run sw",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:cli": "http-server dist/",
    "updatesw": "./updatesw.sh",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js && npm run updatesw",
    "build:aot": "ng build --aot --prod --build-optimizer",
    "build:all": "npm run build:aot && npm run sw",
    "build:github": "ng build --aot --prod --build-optimizer --progress false --base-href \"/was-tutorial/\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@types/howler": "^2.0.3",
    "angular2-useful-swiper": "^4.0.6",
    "core-js": "^2.4.1",
    "howler": "^2.0.4",
    "idb-keyval": "^2.3.0",
    "rxjs": "^5.4.3",
    "web-animations-js": "^2.3.1",
    "wickeyappstore": "^1.9",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/language-service": "^4.4.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "angular2-useful-swiper": "^4.0.6",
    "codelyzer": "~3.1.2",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "sw-precache": "^5.2.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.4"
  }
}
